@{
    ViewData["Title"] = "Index";
}

@model IEnumerable<Tag>

<div class="d-flex justify-content-end mb-3 mr-2">
    <a asp-action="Create" class="btn btn-success"> Create Tag</a>
</div>


<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th>
                                #
                            </th>
                            <th>
                                Name
                            </th>
                            <th>
                                Settings
                            </th>
                        </tr>
                    </thead>
                    <tbody class="tag">
                        
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>


